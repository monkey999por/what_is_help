[core]
	editor = sakura
	autoCRLF = false
	quotepath = false
[user]
	name = monkey999por
	email = 34706709+monkey999por@users.noreply.github.com
[credential]
	helper = manager
[alias]
	a = add
	b = branch
	c = checkout
	ch = cherry-pick
	d = diff
	rmh = reset --mixed HEAD
	rhh = reset --hard HEAD
	s = status
	f = fetch
	l = log --graph --all --format='[%as %ah] %x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(green reverse)%d%Creset%s' --branches
	branch-desc = "!f(){ git config branch.$1.description;};f"
[winUpdater]
	recentlySeenVersion = 2.27.0.windows.1
